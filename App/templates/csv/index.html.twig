{% extends 'base.html.twig' %}

{% block body %}
    <h1>Upload CSV</h1>

    {{ form_start(form) }}
        {{ form_widget(form.csv_file) }}
        <button class="btn btn-primary">Upload</button>
    {{ form_end(form) }}

    {% for message in app.flashes('success') %}
        <div class="alert alert-success">{{ message }}</div>
    {% endfor %}
{% endblock %}
